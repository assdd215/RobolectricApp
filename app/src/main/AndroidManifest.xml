<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.luzeping_sx.robolectricapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.luzeping_sx.robolectricapp.MainActivity">

        </activity>

        <activity android:name="com.example.luzeping_sx.robolectricapp.MyActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.example.luzeping_sx.robolectricapp.MyService"/>

        <receiver android:name=".MyReceiver">
            <intent-filter>
                <action android:name="com.example.luzeping_sx.BRADCAST"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>